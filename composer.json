	"homepage": "http://arckinteractive.com",
	"keywords": [
		"elgg", "plugin", "events"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Matt Beckett",
			"email": "matt@arckinteractive.com"
